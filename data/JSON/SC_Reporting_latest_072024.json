    "paths": {
        "/reporting/v1/content/{product}/{type}/{reportName}/{asOf}": {
            "get": {
                "tags": [
                    "Reporting"
                ],
                "summary": "Download Report",
                "description": "The Download Report API provides the ability to download latest reports such as end of day statements report or general supporting documentation report. It acts as an alternative to SFTP based Host2Host for access to files-based content such as PDFs, DOCX, XLSX, CSV. When successfully executed it returns the HTTP header and the response body as a binary stream of the report files. Applicable Reports - QLDS0010R, E_140, E_139, MT940, MT940A, MT942, CAMT52, CAMT53, MT940-UTF8, MT942-UTF8. Webhook notification will be sent to the client when report is available.",
                "operationId": "getReportContent",
                "parameters": [
                    {
                        "name": "asOf",
                        "in": "path",
                        "description": "Reporting Date - format yyyy-mm-dd",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "product",
                        "in": "path",
                        "description": "Product Category - Collections, Payments, Account",
                        "required": true,
