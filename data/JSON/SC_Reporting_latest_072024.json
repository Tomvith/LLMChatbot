    "components": {
        "schemas": {
            "OpenApiReport": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "description": "Report ID",
                        "example": "d71334c15303"
                    },
                    "dateTimeStamp": {
                        "type": "string",
                        "description": "Report date",
                        "format": "date",
                        "example": "2021-01-21T00:00:00.000Z"
                    },
                    "content": {
                        "type": "string",
                        "description": "Base64 encoded string of report content",
                        "example": "PD94bWwgdmVyc2lvbj0iMS4wIiBlbmNvXGL"
                    }
                },
                "description": "List of Reports"
            },
            "OpenApiReportsResponse": {
                "type": "object",
                "properties": {
                    "groupId": {
                        "type": "string",
                        "description": "Client Registered Group ID"
                    },
                    "reportName": {
                        "type": "string",
                        "description": "Report Name",
                        "example": "CAMT52"
                    },
                    "reports": {
                        "type": "array",
                        "description": "List of Reports",
                        "items": {
                            "$ref": "#/components/schemas/OpenApiReport"
                        }
                    }
                }
            }
