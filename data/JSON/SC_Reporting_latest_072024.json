        "/reporting/v2/content/{product}/{type}/{reportName}/{asOf}/list": {
            "get": {
                "tags": [
                    "Reporting"
                ],
                "summary": "Download single/multiple reports",
                "description": "The Download single/multiple reports API provides an ability to download reports such as end of day statements or general supporting documentation. By default it returns all the reports available for that particular day else filters the reports based on the query parameter fetchReportType. fetchReportType - LATEST returns the latest report for that particular day. fetchReportType - PENDING returns all the pending reports which are not downloaded for that particular day. It acts as an alternative to SFTP based Host2Host for access to file based content such as PDFs, DOCX, XLSX, CSV. When successfully executed it returns the response body as a json object listing all the report details with content as Base64 string.",
                "operationId": "getReportsContent",
                "parameters": [
                    {
                        "name": "asOf",
                        "in": "path",
                        "description": "Reporting Date - format yyyy-mm-dd",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "product",
                        "in": "path",
                        "description": "Product Category - Collections, Payments, Account",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "type",
                        "in": "path",
                        "description": "Product Type - DDA, DDI, Payment Status, Statement ",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "reportName",
                        "in": "path",
                        "description": "Report Name - QLDS0010R, E_140, E_139, MT940, MT940A, MT942, CAMT52, CAMT53, MT940-UTF8, MT942-UTF8",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "fetchReportType",
                        "in": "query",
                        "description": "Fetch Report Type - LATEST, PENDING\nLATEST - Sends the latest report of the day in response\nPENDING - Sends the pending reports which are not downloaded yet in response\nDEFAULT - Sends all the reports available for the particular day in response (Default behaviour of API if there is no query param)",
                        "schema": {
                            "type": "string",
                            "default": "DEFAULT",
                            "enum": [
                                "LATEST",
                                "PENDING",
                                "DEFAULT"
                            ]
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success with data",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/OpenApiReportsResponse"
                                },
                                "example": {
                                    "groupId": "UAASTS1",
                                    "reportName": "CAMT52",
                                    "reports": [
                                        {
                                            "id": "d71334c15303",
                                            "dateTimeStamp": "2021-01-21T00:00:00.000Z",
                                            "content": "PD94bWwgdmVyc2lvbj0iMS4wIiBlbmNvXGL"
                                        }
                                    ]
                                }
                            }
                        }
                    },
                    "201": {
