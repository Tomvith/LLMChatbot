    "components": {
        "schemas": {
            "OpenApiAccountTransactionResponse": {
                "type": "object",
                "properties": {
                    "data": {
                        "$ref": "#/components/schemas/OpenApiAccountTransactionResponseDataArray"
                    },
                    "links": {
                        "type": "object",
                        "properties": {
                            "self": {
                                "type": "string",
                                "description": "URIs which are typically links on the web"
                            }
                        }
                    }
                }
            },
            "OpenApiAccountTransactionResponseDataArray": {
                "type": "object",
                "properties": {
                    "transaction": {
                        "type": "array",
                        "description": "Transaction Details",
                        "items": {
                            "$ref": "#/components/schemas/OpenApiAccountTransactionResponseData"
                        }
                    }
                }
            },
            "OpenApiAccountTransactionResponseData": {
                "type": "object",
                "required": [
                    "accountId",
                    "accountNumber",
                    "transactionCode",
                    "transactionId",
                    "transactionDescription",
                    "datetime",
                    "creditDebitIndicator",
                    "status",
                    "amount",
                    "currency",
                    "balance"
                ],
                "properties": {
                    "accountId": {
                        "type": "string",
                        "maxLength": 60,
                        "example": "34313130393239313130353A5343424C484B48485858583A484B44",
                        "description": "Unique identifier used internally by the bank to identify an account"
                    },
                    "accountNumber": {
                        "type": "string",
                        "maxLength": 11,
                        "example": "XXXXXXXXXX02",
                        "description": "Account number assigned by the bank to identify an account"
                    },
                    "transactionCode": {
                        "type": "string",
                        "maxLength": 5,
                        "example": "PYMT",
                        "description": "ISO transaction code"
                    },
                    "transactionId": {
                        "type": "string",
                        "maxLength": 16,
                        "example": "PBKBP62508440005099",
                        "description": "Unique identifier for the transaction used by the bank"
                    },
                    "transactionDescription": {
                        "type": "string",
                        "maxLength": 100,
                        "example": "Bill Payment",
                        "description": "Description of the transaction"
                    },
                    "transactionDate": {
                        "type": "string",
                        "maxLength": 10,
                        "example": "2020-12-22T00:00:00.000Z",
                        "description": "Date of the transaction"
                    },
                    "creditDebitIndicator": {
                        "type": "string",
                        "maxLength": 6,
                        "example": "Debit",
                        "description": "Indicates whether the transaction is a credit or debit entry"
                    },
                    "status": {
                        "type": "string",
                        "maxLength": 10,
                        "example": "Pending",
                        "description": "Status of the transaction entry"
                    },
                    "amount": {
                        "type": "string",
                        "maxLength": 20,
                        "example": 10079.36,
                        "description": "Transaction amount"
                    },
                    "currency": {
                        "type": "string",
                        "minLength": 0,
                        "maxLength": 3,
                        "example": "HKD",
                        "description": "Account currency code"
                    },
                    "currencyExchange": {
                        "description": "Currency exchange object for a foreign currency transaction",
                        "$ref": "#/components/schemas/OpenApiCurrencyExchange"
                    },
                    "balance": {
                        "description": "Balance object of the account",
                        "$ref": "#/components/schemas/OpenApiBalance"
                    }
                }
            },
            "OpenApiCurrencyExchange": {
                "type": "object",
                "required": [
                    "amount",
                    "currency",
                    "rate",
                    "date"
                ],
                "properties": {
                    "amount": {
                        "type": "string",
                        "maxLength": 20,
                        "example": 1300,
                        "description": "Transaction amount in foreign currency"
                    },
                    "currency": {
                        "type": "string",
