    "paths": {
        "/hkopenbanking/accounts/v2/transactions": {
            "get": {
                "tags": [
                    "Account Services"
                ],
                "summary": "Hong Kong – Account Transaction",
                "description": "The Hong Kong - Account Transaction API enables the registered Third-Party Service Provider (TSP) to securely retrieve authorised Standard Chartered’s customer account transaction details information. It allows user to retrieve account transaction details using different filtering criteria - latest records since the last statement or a time frame with start and/or end date. This API will provide details for a list of accounts for a specified date range.",
                "operationId": "account-transactions",
                "parameters": [
                    {
                        "name": "showLatest",
                        "in": "query",
                        "description": "Indicates to retrieve the latest transaction records since the last statement. If it is set as True, fromDateTime and toDateTime will not be applicable",
                        "required": false,
                        "default": true,
                        "schema": {
                            "type": "boolean"
                        }
                    },
                    {
                        "name": "fromDateTime",
                        "in": "query",
                        "description": "Start date and time for filtering out the transactions. Applicable only if showLatest is False. ISO format of DD-MM-YYYY'T'HH24:MI:SS (HKT)",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "toDateTime",
                        "in": "query",
                        "description": "End date and time for filtering out the transactions. Mandatory if fromDateTime is not blank. ISO format of DD-MM-YYYY'T'HH24:MI:SS (HKT)",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/OpenApiAccountTransactionResponse"
                                },
                                "example": "{\n   \"data\": {\n      \"transaction\": [\n         {\n            \"accountId\": \"34313130393239313130353A5343424C484B48485858583A484B44\",\n            \"accountNumber\": \"01XXXXX188\",\n            \"transactionCode\": \"PYMT\",\n            \"transactionId\": \" PBKBP62508440005099\",\n            \"transactionDescription\": \"Bill Payment\",\n            \"transactionDate\": \"2020-12-22\",\n            \"creditDebitIndicator\": \"Debit\",\n            \"status\": \"Booked\",\n            \"amount\": \"10079.36\",\n            \"currency\": \"HKD\",\n            \"currencyExchange\": {\n               \"amount\": \"1300\",\n               \"currency\": \"USD\",\n               \"rate\": \"7.75\",\n               \"date\": \"2020-12-22\"\n            },\n            \"balance\": {\n               \"type\": \"OpeningBalance\",\n               \"creditDebitIndicator\": \"Debit\",\n               \"amount\": \"1920.64\",\n               \"currency\": \"HKD\",\n               \"datetime\": \"2020-12-22T07:06:20\"\n            }\n         },\n         {\n            \"accountId\": \"34313130393239313130353A5343424C484B48485858583A484B44\",\n            \"accountNumber\": \"01XXXXX188\",\n            \"transactionCode\": \"INTR\",\n            \"transactionId\": \" MBKFT42508230004073\",\n            \"transactionDescription\": \"Fund Transfer\",\n            \"transactionDate\": \"2020-12-22\",\n            \"creditDebitIndicator\": \"Debit\",\n            \"status\": \"Booked\",\n            \"amount\": \"1000\",\n            \"currency\": \"HKD\",\n            \"balance\": {\n               \"type\": \"CurrentBalance\",\n               \"creditDebitIndicator\": \"Debit\",\n               \"amount\": \"920.64\",\n               \"currency\": \"HKD\",\n               \"datetime\": \"2020-12-22T07:06:20\"\n            }\n         }\n      ]\n   },\n   \"links\": {\n      \"self\": \"string\"\n   }\n}"
                            }
                        }
                    },
                    "201": {
                        "description": "Success. Data created"
                    },
                    "202": {
                        "description": "Success. Data accepted"
                    },
                    "204": {
                        "description": "Success. No data returned"
                    },
                    "400": {
                        "description": "Invalid request. Missing or invalid data"
                    },
                    "401": {
                        "description": "Unauthorised request"
                    },
                    "403": {
                        "description": "Authentication failed"
                    },
                    "404": {
                        "description": "Resource not found"
                    },
                    "429": {
                        "description": "Default return code for health status, indicating a warning"
                    },
                    "500": {
                        "description": "Internal server error. This indicates an error in the API server"
                    },
                    "503": {
                        "description": "System dependency is down or undergoing maintenance. Try again later"
                    }
                },
                "x-codegen-request-body-name": "body",
                "x-markets": "HK"
            }
        },
        "/hkopenbanking/accounts/v2/{accountId}/transactions": {
            "get": {
                "tags": [
                    "Account Services"
                ],
                "summary": "Hong Kong – Account Transaction by Account Number",
                "description": "The Hong Kong - Account Transaction by Account Number API enables the registered Third-Party Service Provider (TSP) to securely retrieve authorised Standard Chartered’s customer account transaction details information based on the indicated Account Number. It allows user to retrieve account transaction details using different filtering criteria - latest records since the last statement or a time frame with start and/or end date. This API will provide details for a specific account  for a specified date range.",
                "operationId": "account-transactions-by-accountId",
                "parameters": [
                    {
                        "name": "accountId",
                        "in": "path",
                        "description": "Account Number for which transaction history is requested",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "showLatest",
                        "in": "query",
                        "description": "Indicates to retrieve the latest transaction records since the last statement. If it is set as True, fromDateTime and toDateTime will not be applicable",
                        "required": false,
                        "default": true,
                        "schema": {
                            "type": "boolean"
                        }
                    },
                    {
                        "name": "fromDateTime",
                        "in": "query",
                        "description": "Start date and time for filtering out the transactions. Applicable only if showLatest is False. ISO format of DD-MM-YYYY'T'HH24:MI:SS (HKT)",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "toDateTime",
                        "in": "query",
                        "description": "End date and time for filtering out the transactions. Mandatory if fromDateTime is not blank. ISO format of DD-MM-YYYY'T'HH24:MI:SS (HKT)",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/OpenApiAccountTransactionResponse"
                                },
                                "example": "{\n   \"data\": {\n      \"transaction\": [\n         {\n            \"accountId\": \"34313130393239313130353A5343424C484B48485858583A484B44\",\n            \"accountNumber\": \"01XXXXX188\",\n            \"transactionCode\": \"PYMT\",\n            \"transactionId\": \" PBKBP62508440005099\",\n            \"transactionDescription\": \"Bill Payment\",\n            \"transactionDate\": \"2020-12-22\",\n            \"creditDebitIndicator\": \"Debit\",\n            \"status\": \"Booked\",\n            \"amount\": \"10079.36\",\n            \"currency\": \"HKD\",\n            \"currencyExchange\": {\n               \"amount\": \"1300\",\n               \"currency\": \"USD\",\n               \"rate\": \"7.75\",\n               \"date\": \"2020-12-22\"\n            },\n            \"balance\": {\n               \"type\": \"CurrentBalance\",\n               \"creditDebitIndicator\": \"Debit\",\n               \"amount\": \"1920.64\",\n               \"currency\": \"HKD\",\n               \"datetime\": \"2020-12-22T07:06:20\"\n            }\n         },\n         {\n            \"accountId\": \"34313130393239313130353A5343424C484B48485858583A484B44\",\n            \"accountNumber\": \"01XXXXX188\",\n            \"transactionCode\": \"INTR\",\n            \"transactionId\": \" MBKFT42508230004073\",\n            \"transactionDescription\": \"Fund Transfer\",\n            \"transactionDate\": \"2020-12-22\",\n            \"creditDebitIndicator\": \"Debit\",\n            \"status\": \"Booked\",\n            \"amount\": \"1000\",\n            \"currency\": \"HKD\",\n            \"balance\": {\n               \"type\": \"CurrentBalance\",\n               \"creditDebitIndicator\": \"Debit\",\n               \"amount\": \"920.64\",\n               \"currency\": \"HKD\",\n               \"datetime\": \"2020-12-22T07:06:20\"\n            }\n         }\n      ]\n   },\n   \"links\": {\n      \"self\": \"string\"\n   }\n}"
                            }
                        }
                    },
                    "201": {
                        "description": "Success. Data created"
                    },
                    "202": {
                        "description": "Success. Data accepted"
                    },
                    "204": {
                        "description": "Success. No data returned"
                    },
                    "400": {
                        "description": "Invalid request. Missing or invalid data"
                    },
                    "401": {
                        "description": "Unauthorised request"
                    },
                    "403": {
                        "description": "Authentication failed"
                    },
                    "404": {
                        "description": "Resource not found"
                    },
                    "429": {
                        "description": "Default return code for health status, indicating a warning"
                    },
                    "500": {
                        "description": "Internal server error. This indicates an error in the API server"
                    },
                    "503": {
                        "description": "System dependency is down or undergoing maintenance. Try again later"
                    }
                },
                "x-codegen-request-body-name": "body",
                "x-markets": "HK"
            }
        },
