    "components": {
        "schemas": {
            "OpenApiTrackerStatuses": {
                "type": "object",
                "properties": {
                    "trackerStatuses": {
                        "type": "array",
                        "description": "Status Details",
                        "items": {
                            "$ref": "#/components/schemas/OpenApiTrackerStatus"
                        }
                    }
                }
            },
            "OpenApiPaymentEventCharges": {
                "type": "object",
                "properties": {
                    "amount": {
                        "type": "number",
                        "format": "double",
                        "description": "Amount",
                        "example": "10.0"
                    },
                    "currency": {
                        "type": "string",
                        "description": "Currency",
                        "example": "USD"
                    }
                }
            },
            "OpenApiTrackerStatus": {
                "type": "object",
                "properties": {
                    "uetr": {
                        "maxLength": 36,
                        "type": "string",
                        "description": "uetr",
                        "example": "12026920-3dfe-4863-a145-6f5685717aaa"
                    },
                    "paymentEvent": {
                        "type": "array",
                        "description": "Payment Event",
                        "items": {
                            "$ref": "#/components/schemas/OpenApiPaymentEvent"
                        }
                    }
                }
            },
            "OpenApiTrackerRequest": {
                "required": [
                    "uetrs"
                ],
                "type": "object",
                "properties": {
                    "uetrs": {
                        "maxItems": 200,
                        "type": "array",
                        "description": "uetrs",
                        "items": {
                            "type": "string"
                        },
                        "example": [
                            "12026920-3dfe-4863-a145-6f5685717aaa",
                            "5aa6b593-b715-482f-a6b5-93ababababab"
                        ]
                    },
                    "since": {
                        "type": "string",
                        "description": "Date",
                        "example": "2022-06-10T13:21:02.000+0000"
                    }
                }
            },
            "OpenApiPaymentEvent": {
                "type": "object",
                "properties": {
                    "paymentReferenceNumber": {
                        "type": "string",
                        "description": "Payment Reference Number",
                        "example": "abc123"
                    },
                    "from": {
                        "type": "string",
                        "description": "From",
                        "example": "BANABEBBXXX"
                    },
                    "to": {
                        "type": "string",
                        "description": "To",
                        "example": "BANBUS33XXX"
                    },
                    "paymentStatus": {
                        "type": "string",
                        "description": "Payment Status",
                        "example": "ACSP"
                    },
                    "paymentStatusReason": {
                        "type": "string",
                        "description": "Payment Status Reason",
                        "example": "G000"
                    },
                    "paymentStatusDesc": {
                        "type": "string",
                        "description": "Payment Status Description",
                        "example": "Pushed to GPI Agent"
                    },
                    "statusDateTime": {
                        "type": "string",
                        "description": "Status Date Time",
                        "example": "2022-06-10T09:55:53.000+0000"
                    },
                    "transactionDateTime": {
