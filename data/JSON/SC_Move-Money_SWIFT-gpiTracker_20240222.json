                        "example": "2022-06-10T09:55:53.000+0000"
                    },
                    "settlementMethod": {
                        "type": "string",
                        "description": "Settlement Method",
                        "example": "INDA"
                    },
                    "sourceCurrency": {
                        "type": "string",
                        "description": "Source Currency",
                        "example": "USD"
                    },
                    "targetCurrency": {
                        "type": "string",
                        "description": "Target Currency",
                        "example": "SGD"
                    },
                    "settledCurrency": {
                        "type": "string",
                        "description": "Settled Currency",
                        "example": "USD"
                    },
                    "exchangeRate": {
                        "type": "number",
                        "format": "double",
                        "description": "Exchange Rate",
                        "example": 1.4
                    },
                    "chargeBearer": {
                        "type": "string",
                        "description": "Charge Bearer",
                        "example": "SHAR"
                    },
                    "settledAmount": {
                        "type": "number",
                        "format": "double",
                        "description": "Settled Amount",
                        "example": 1000
                    },
                    "chargeAmount": {
                        "type": "number",
                        "format": "double",
                        "description": "Charge Amount",
                        "example": 12
                    },
                    "clearingSystem": {
                        "type": "string",
                        "description": "Clearing System",
                        "example": "FW"
                    },
                    "charges": {
                        "type": "array",
                        "description": "Charges",
                        "items": {
                            "$ref": "#/components/schemas/OpenApiPaymentEventCharges"
